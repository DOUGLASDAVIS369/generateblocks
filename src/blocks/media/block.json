{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "generateblocks/media",
    "title": "Media",
    "category": "generateblocks",
    "description": "A self-closing media element.",
    "keywords": [ "element", "container", "group" ],
    "version": "1.0.0",
    "textdomain": "generateblocks",
    "attributes": {
        "uniqueId": {
            "type": "string"
        },
		"tagName": {
			"type": "string",
			"default": "",
			"enum": [
				"img"
			]
		},
		"styles": {
			"type": "object"
		},
		"css": {
			"type": "string"
		},
		"globalClasses": {
			"type": "array"
		},
		"htmlAttributes": {
			"type": "object"
		}
    },
    "supports": {
        "align": false,
		"className": false
    },
    "editorScript": "file:./index.js",
    "editorStyle": [ "file:./index.css", "generateblocks-block-styles", "generateblocks-components" ],
	"usesContext": [ "postId" ]
}
